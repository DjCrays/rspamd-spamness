<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>{41a9ee8a-e9c6-4076-84ac-bc1b612dca68}</em:id>
    <em:name>Spamness</em:name>
    <em:version>0.1</em:version>
    
    <!-- Thunderbird -->
    <em:targetApplication>
      <Description>
        <em:id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</em:id>
        <em:minVersion>2.0</em:minVersion>
        <em:maxVersion>2.0.0.*</em:maxVersion>
      </Description>
    </em:targetApplication>
    
    <!-- The contents of the extension //-->
    <em:file>
      <Description about="urn:mozilla:extension:file:spamness.jar">
        <em:package>content/spamness/</em:package>
	<em:locale>locale/en-US/spamness/</em:locale>
        <em:skin>skin/classic/spamness/</em:skin>
      </Description>
    </em:file>

    <!-- Front End Integration Hooks (used by Extension Manager)-->
    <em:description>Gives a color- and size-based visual measurement of how spam-like an individual message is based on its normalized SpamAssassin score.</em:description>
    <em:creator>Ryan Lee (ryan@ryanlee.org)</em:creator>
    <em:iconURL>chrome://spamness/skin/icon32x32.png</em:iconURL>
    <em:homepageURL>http://ryanlee.org/software/mozilla/thunderbird/spamness/</em:homepageURL>
    <em:type parseType="Integer">2</em:type> <!-- type=extension -->
  </Description>
</RDF>
